import "lib.fg"
const list: [8]string = [ "a", "b", "c", "d", "e", "6", "4", "5"];
const text: string = "Words are cool";
fn main(): void {
  var s: string = list[2];
  sys::writeChar(s[0]);
  sys::writeChar(' ');

  const len: u8 = text[-1] as u8;
  for (var q: u8 = 0; q < len; q = q + 1) {
    sys::writeChar(text[q]);
  }
}

