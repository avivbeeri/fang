import "lib.fg"

type Bundle {
  alive: bool;
  s: string;
  id: i8;
  test: [5]char;
}

fn main(): void {
  var s: Bundle = {
    id = 5;
    test = ['a', 'b', 'c', 'd', 'e'];
    s = "Hello world"
  };

  sys::writeChar(' ');
  sys::writeI8(s.id);
  sys::writeChar(' ');
  sys::write(s.test, 5);
  sys::writeChar(' ');
  sys::write(s.s as ^char, 11);
}

