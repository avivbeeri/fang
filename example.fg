fn syscall_write(c: char): void {
  asm {
    "MOV X0, #1" // STDOUT
    "ADD X1, FP, #16" // PTR (FP+16) // function argument
    "MOV X2, 1"
    "MOV X16, #4"
    "SVC 0"
   };
}

fn main(): i8 {
  var c: char = 'a';
  var p: ^char = ^c;
  var g: [5]char;
  
  for (var i: i8 = 0; i < (1 << 4); i = i + 1) {
    @p = (65 + i) as char;
    write(@p);
  }
  return 0;
}

fn write(c : char): void {
  syscall_write(c);
}
