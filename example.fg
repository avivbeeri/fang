import "lib.fg"

type Test {
  s: sys::Sprite;
}

union Face = sys::Sprite | Test;
var g: Face;

bank test {
  fn goodbye(): void {
    sys::writeChar('C');
    sys::writeChar('\n');
  }
}

fn main(): void {
  hello();
  goodbye();
  (g as Test).s.id = 5; 
  sys::writeChar('\n');
  sys::writeU8((g as Test).s.id);
}

bank newtest {
  const i: i8 = 42;
  fn hello(): void {
    sys::writeChar('B');
    sys::writeChar('\n');
    sys::writeI8(i);
  }
}


