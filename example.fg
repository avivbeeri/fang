import "lib.fg"

type Bundle {
  s: string;
  alive: bool;
  id: i8;
  test: [5]char;
}

fn construct(): Bundle {
  var s: Bundle = {
    s = "hello world";
    alive = true;
    id = 42;
  };
  return s;
}

fn main(): void {
  var b: Bundle = construct();
  var q: Bundle;
  q = construct();

  sys::writeChar('\'');
  sys::writeI8(b.id);
  sys::writeChar('\'');
  sys::writeI8(q.id);
  sys::writeChar('\'');
}

